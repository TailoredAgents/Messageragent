{
  "name": "junkquote-agent-service",
  "version": "0.1.0",
  "description": "Messenger-first JunkQuoteAgent MVP built on OpenAI Agents SDK.",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prebuild": "prisma generate",
    "build": "npm run prisma:generate",
    "dev": "tsx watch src/index.ts",
    "start": "npm run prisma:migrate:deploy && tsx src/index.ts",
    "start:worker": "npm run prisma:migrate:deploy && tsx src/worker.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "simulate:messenger": "tsx scripts/simulate-messenger.ts"
  },
  "dependencies": {
    "@fastify/formbody": "^8.0.2",
    "@fastify/static": "^8.3.0",
    "@fastify/view": "^10.0.2",
    "@openai/agents": "^0.2.1",
    "@prisma/client": "^6.18.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "fastify": "^5.6.1",
    "luxon": "^3.7.2",
    "nodemailer": "^6.9.15",
    "openai": "^6.7.0",
    "tsx": "^4.19.1",
    "twilio": "^5.10.4",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.8.1",
    "@types/nodemailer": "^6.4.14",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "vitest": "^2.1.4"
  }
}
